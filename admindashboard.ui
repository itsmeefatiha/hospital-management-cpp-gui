   <string notr="true">/* --- STYLE GLOBAL --- */
QMainWindow { background-color: #f0f2f5; }
QWidget { font-family: &quot;Segoe UI&quot;, sans-serif; }

/* --- SIDEBAR --- */
QFrame#frame_sidebar { background-color: white; border-right: 1px solid #e0e0e0; }
QLabel#label_app_name { color: #2d7aed; font-weight: bold; font-size: 20px; padding: 10px; }

/* BOUTONS SIDEBAR - CORRIG√â */
QPushButton[class=&quot;SidebarBtn&quot;] { 
	text-align: left; 
	padding: 12px 20px; 
	border: none; 
	background-color: transparent; 
	font-size: 15px; 
	color: #555; 
	border-radius: 5px; 
	margin: 2px 10px; 
}
QPushButton[class=&quot;SidebarBtn&quot;]:hover { background-color: #f5f7fa; color: #2d7aed; }
QPushButton[class=&quot;SidebarBtn&quot;]:checked { background-color: #e6f0ff; color: #2d7aed; font-weight: bold; }

/* --- CARTES - CORRIG√â --- */
QFrame[class=&quot;Card&quot;] { background-color: white; border-radius: 8px; border: 1px solid #e0e0e0; }

/* LABELS STATISTIQUES - CORRIG√â */
QLabel[class=&quot;StatValue&quot;] { font-size: 32px; font-weight: bold; color: #2d7aed; }
QLabel[class=&quot;StatLabel&quot;] { color: #777; font-size: 14px; }
QLabel[class=&quot;SectionTitle&quot;] { font-size: 18px; font-weight: bold; color: #333; }

/* --- TABLEAU --- */
QTableWidget { 
    background-color: white; 
    border: none; 
    gridline-color: #f0f0f0; 
    selection-background-color: #e6f0ff; 
    selection-color: #2d7aed; 
    color: #333333;
}

QHeaderView::section { 
    background-color: #f8f9fa;
    padding: 8px; 
    border: none; 
    border-bottom: 2px solid #2d7aed;
    font-weight: bold; 
    color: #333333;
}

QTableWidget::item {
    padding: 5px;
}

/* Style temporaire pour les placeholders */
QWidget#chart_placeholder, QWidget#doughnut_placeholder {
    background-color: #fafafa;
    border: 1px dashed #d0d0d0;
    border-radius: 4px;
}
